{
    "name": "codewithsushil/learn-pest",
    "description": "Learn testing with Pest 4",
    "license":"MIT",
    "autoload":{
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4":{
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^4.1"
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "carthage-software/mago": true,
            "pestphp/pest-plugin": true
        }
    }
}
